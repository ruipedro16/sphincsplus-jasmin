inline fn memset_u8(
    reg u64 a inlen value
)
{
  reg u64 i;
  
  i = 0;
  while (i < inlen)
  {
    [a + i] = value;
    i += 1;
  }
}

// copies INLEN bytes from memory area src to memory area dest.
inline fn __memmove_u8(
  reg u64 dest src size
) 
{
  reg u64 i;
  reg u8 t;

  i = 0;
  while (i < size) {
    t = [src + i];
  }
}
