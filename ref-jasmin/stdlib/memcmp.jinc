// Returns 0 if a and b are equal
// Returns -1 otherwise
inline fn __memcmp<INLEN>(reg ptr u8[INLEN] a b) -> reg u8 {
    reg u64 i;
    reg u8 t acc;

    acc = 0;
  
    i = 0;
    while (i < XMSS_N) {
        t = a[i];
        t ^= b[i];
        acc |= t;
        i += 1;
    }

    return acc;
}//<>
